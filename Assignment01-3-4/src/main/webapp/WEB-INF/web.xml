<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
<display-name>Archetype Created Web Application</display-name>
<context-param>
	<param-name>contextParameter</param-name>
	<param-value>Class is learning</param-value>
</context-param>
<servlet>
	<servlet-name>FirstServlet</servlet-name>
	<display-name>FirstServlet</display-name>
	<description></description>
	<servlet-class>FirstServlet</servlet-class>
	<init-param>
		<param-name>firstParam</param-name>
		<param-value>This is from first Servlet</param-value>
	</init-param>
</servlet>
<servlet>
	<servlet-name>SecondServlet</servlet-name>
	<display-name>SecondServlet</display-name>
	<description></description>
	<servlet-class>SecondServlet</servlet-class>
	<init-param>
		<param-name>secondParam</param-name>
		<param-value>This is from second Servlet</param-value>
	</init-param>
</servlet>
<servlet>
	<servlet-name>MainServlet</servlet-name>
	<display-name>MainServlet</display-name>
	<description></description>
	<servlet-class>MainServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>ThirdServlet</servlet-name>
	<display-name>ThirdServlet</display-name>
	<description></description>
	<servlet-class>ThirdServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>FirstServlet</servlet-name>
	<url-pattern>/FS</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>SecondServlet</servlet-name>
	<url-pattern>/SS</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>MainServlet</servlet-name>
	<url-pattern>/MS</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>ThirdServlet</servlet-name>
	<url-pattern>/TS</url-pattern>
</servlet-mapping>

  <error-page>
  <exception-type>java.lang.NumberFormatException</exception-type>
  <location>/error.html</location>
  </error-page>
</web-app>
