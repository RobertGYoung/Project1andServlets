<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterUserServlet</servlet-name>
  	<display-name>RegisterUserServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.RegisterUserServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginUserServlet</servlet-name>
  	<display-name>LoginUserServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.LoginUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterUserServlet</servlet-name>
  	<url-pattern>/RUS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginUserServlet</servlet-name>
  	<url-pattern>/LUS</url-pattern>
  </servlet-mapping>
</web-app>
