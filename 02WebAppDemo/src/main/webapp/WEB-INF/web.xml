<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FirstServlet</servlet-name>
  	<display-name> FirstServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.myservlets.FirstServlet</servlet-class>
  	<load-on-startup>1</load-on-startup>

  	
  </servlet>
    <servlet>
  	<servlet-name>SecondServlet</servlet-name>
  	<display-name>SecondServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.myservlets.SecondServlet</servlet-class>
  	  	<load-on-startup>1</load-on-startup>
  	
  </servlet>
    <servlet>
    	<servlet-name>ThirdServlet</servlet-name>
    	<display-name>ThirdServlet</display-name>
    	<description></description>
    	<servlet-class>com.revature.myservlets.ThirdServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>First</servlet-name>
    	<display-name>First</display-name>
    	<description></description>
    	<servlet-class>com.revature.servlets.First</servlet-class>
    </servlet>

    <servlet>
    	<servlet-name>GetAllUsersServlet</servlet-name>
    	<display-name>GetAllUsersServlet</display-name>
    	<description></description>
    	<servlet-class>com.revature.myservlets.GetAllUsersServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>UpdateUserServlet</servlet-name>
    	<display-name>UpdateUserServlet</display-name>
    	<description></description>
    	<servlet-class>com.revature.myservlets.UpdateUserServlet</servlet-class>
    </servlet>


  <servlet-mapping>
  	<servlet-name>FirstServlet</servlet-name>
  	<url-pattern>/FS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SecondServlet</servlet-name>
  	<url-pattern>/SS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ThirdServlet</servlet-name>
  	<url-pattern>/TS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>First</servlet-name>
  	<url-pattern>/F</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetAllUsersServlet</servlet-name>
  	<url-pattern>/GAUS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateUserServlet</servlet-name>
  	<url-pattern>/UU</url-pattern>
  </servlet-mapping>


  
  <welcome-file-list>
  <welcome-file>HelloServlet.html</welcome-file>
  </welcome-file-list>

<error-page>
	<exception-type>java.lang.NullPointerException</exception-type>
	<location>/error.html</location>
	
</error-page>
</web-app>
